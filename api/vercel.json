{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "FE/ShopThuCong/vite.config.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "FE/ShopThuCong/dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "FE/ShopThuCong/dist/$1" }
  ]
}
